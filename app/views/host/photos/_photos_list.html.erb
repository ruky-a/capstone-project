<div class="container">
<div class="row">
  <% @room.photos.each do |photo| %>
  <% if photo.images.attached? %>
  <% (0...photo.images.count).each do |image| %>
<div class="col-md-4">
<div class="card test-white">
<div class="card-header preview">
  <%= image_tag(photo.thumbnail(image)) %>
</div>
<div class="card-body">
  <%= photo.caption %>
<span class="pull-right">
<%= link_to host_room_photo_path(photo.room_id, photo), remote: true, method: :delete, data: {confirm: "Are you sure you want to delete?"} do %>
<i class="fa fa-trash-o" aria-hidden="true"></i>
<% end %>
</span>
</div>
</div>
</div>
<% end %>
<% end %>
<% end %>
</div>
</div>
</div>


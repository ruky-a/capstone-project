 
<div class="box">  
   <div class="row">
    <div class="col-6 offset-3">
    <%= image_tag avatar_url(@user), class: "avatar2" %>
  </div>
</div>
  <div class="col-6 offset-3"><br />
  <h2><%= @user.fullname %></h2>
  <h6><%= @user.email %></h6>
